<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".MainActivity2">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <ImageView
            android:id="@+id/imageView"
            android:layout_width="135dp"
            android:layout_height="120dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="16dp"
            android:src="@drawable/photo1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="110dp"
            android:src="@drawable/group_15"
            app:layout_constraintBottom_toBottomOf="@+id/imageView"
            app:layout_constraintStart_toStartOf="@+id/imageView"
            app:layout_constraintTop_toTopOf="@+id/imageView" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="175dp"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="70dp"
            android:text="Angelica Jackson"
            android:textColor="@color/text_color"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView" />


        <TextView
            android:id="@+id/textView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:text="Analyzer"
            android:textColor="@color/text_color"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/textView2"
            app:layout_constraintTop_toBottomOf="@+id/textView2" />


        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Change profile"
            android:textColor="@color/text_color"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@+id/textView"
            app:layout_constraintTop_toBottomOf="@+id/textView" />


        <TextView
            android:id="@+id/textView3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:text="Strong side:"
            android:textColor="#595085"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="@+id/imageView"
            app:layout_constraintTop_toBottomOf="@+id/imageView" />

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginTop="8dp"
            app:cardBackgroundColor="#F5F9F9"
            app:cardCornerRadius="8dp"
            app:layout_constraintStart_toStartOf="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/textView3">


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="Analytics"
                android:textColor="#479696"
                android:textSize="12sp" />
        </androidx.cardview.widget.CardView>


        <androidx.cardview.widget.CardView
            android:id="@+id/cardView2"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            app:cardBackgroundColor="#F5F9F9"
            app:cardCornerRadius="8dp"
            app:layout_constraintStart_toEndOf="@+id/cardView"
            app:layout_constraintTop_toBottomOf="@+id/textView3">


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="Perfectionism"
                android:textColor="#479696"
                android:textSize="12sp" />
        </androidx.cardview.widget.CardView>


        <androidx.cardview.widget.CardView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            app:cardBackgroundColor="#F5F9F9"
            app:cardCornerRadius="8dp"
            app:layout_constraintStart_toEndOf="@+id/cardView2"
            app:layout_constraintTop_toBottomOf="@+id/textView3">


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="Analytics"
                android:textColor="#479696"
                android:textSize="12sp" />
        </androidx.cardview.widget.CardView>


        <TextView
            android:id="@+id/weak"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="Weak side:"
            android:textColor="#595085"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="@+id/imageView"
            app:layout_constraintTop_toBottomOf="@+id/cardView" />


        <androidx.cardview.widget.CardView
            android:id="@+id/perf"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginTop="8dp"
            app:cardBackgroundColor="#FFF4F4"
            app:cardCornerRadius="8dp"
            app:layout_constraintStart_toStartOf="@id/weak"
            app:layout_constraintTop_toBottomOf="@+id/weak">


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="Perfectionism"
                android:textColor="#FF7E73"
                android:textSize="12sp" />
        </androidx.cardview.widget.CardView>


        <androidx.cardview.widget.CardView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            app:cardBackgroundColor="#FFF4F4"
            app:cardCornerRadius="8dp"
            app:layout_constraintStart_toEndOf="@id/perf"
            app:layout_constraintTop_toBottomOf="@+id/weak">


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="Analytics"
                android:textColor="#FF7E73"
                android:textSize="12sp" />
        </androidx.cardview.widget.CardView>


        <TextView
            android:id="@+id/repo"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:text="My Reports:"
            android:textColor="#595085"
            app:layout_constraintStart_toStartOf="@+id/imageView"
            app:layout_constraintTop_toBottomOf="@id/perf" />


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/repo"
           >

            <androidx.cardview.widget.CardView
                android:id="@+id/card1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:elevation="0dp"
                app:cardBackgroundColor="#F6F5FB"
                app:cardCornerRadius="16dp"
                app:layout_constraintHorizontal_chainStyle="spread_inside"
                app:layout_constraintStart_toStartOf="@id/imageView"
                app:layout_constraintTop_toBottomOf="@id/repo">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/user"
                        android:layout_width="16dp"
                        android:layout_height="16dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="20dp"
                        android:src="@drawable/user__8__1"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <ImageView
                        android:id="@+id/imageView3"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_marginEnd="20dp"
                        android:src="@drawable/vector"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/user_t"
                        android:layout_width="110dp"
                        android:layout_height="30dp"
                        android:layout_marginTop="10dp"
                        android:text="Astro-psychological report"
                        android:textColor="#403572"
                        android:textSize="11sp"
                        app:layout_constraintStart_toStartOf="@id/user"
                        app:layout_constraintTop_toBottomOf="@id/user" />

                    <TextView
                        android:id="@+id/user_t_2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="Some short description of this type of report."
                        android:textColor="#403572"
                        android:textSize="10sp"
                        app:layout_constraintEnd_toEndOf="@+id/user_t"
                        app:layout_constraintStart_toStartOf="@id/user_t"
                        app:layout_constraintTop_toBottomOf="@id/user_t" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:elevation="0dp"
                app:cardBackgroundColor="#F6F5FB"
                app:cardCornerRadius="16dp"
                app:layout_constraintHorizontal_chainStyle="spread_inside"
                app:layout_constraintStart_toEndOf="@+id/card1"
                app:layout_constraintTop_toTopOf="@id/card1">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/userq"
                        android:layout_width="16dp"
                        android:layout_height="16dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="20dp"
                        android:src="@drawable/calendar_1"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:tint="#479696" />

                    <ImageView
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_marginEnd="20dp"
                        android:src="@drawable/vector"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:tint="#479696" />

                    <TextView
                        android:id="@+id/user_tt"
                        android:layout_width="100dp"
                        android:layout_height="30dp"
                        android:layout_marginTop="10dp"
                        android:text="Monthly prediction report"
                        android:textColor="#479696"
                        android:textSize="12sp"
                        app:layout_constraintStart_toStartOf="@id/userq"
                        app:layout_constraintTop_toBottomOf="@id/userq" />

                    <TextView
                        android:id="@+id/user_t_22"
                        android:layout_width="110dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:text="Some short description of this type of report."
                        android:textColor="#479696"
                        android:textSize="10sp"
                        app:layout_constraintEnd_toEndOf="@+id/user_tt"
                        app:layout_constraintStart_toStartOf="@id/user_tt"
                        app:layout_constraintTop_toBottomOf="@id/user_tt" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/card3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:elevation="0dp"
                app:cardCornerRadius="16dp"
                app:layout_constraintHorizontal_chainStyle="spread_inside"
                app:layout_constraintStart_toStartOf="@id/card1"
                app:layout_constraintTop_toBottomOf="@id/card1">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/userqq"
                        android:layout_width="16dp"
                        android:layout_height="16dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="20dp"
                        android:src="@drawable/calendar_1"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:tint="#FF5D50" />

                    <ImageView
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_marginEnd="20dp"
                        android:src="@drawable/vector"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:tint="#FF5D50" />

                    <TextView
                        android:id="@+id/user_ttt"
                        android:layout_width="110dp"
                        android:layout_height="30dp"
                        android:layout_marginTop="10dp"
                        android:text="Monthlyc  prediction report"
                        android:textColor="#FF5D50"
                        android:textSize="12sp"
                        app:layout_constraintStart_toStartOf="@id/userqq"
                        app:layout_constraintTop_toBottomOf="@id/userqq" />

                    <TextView
                        android:id="@+id/user_t_222"
                        android:layout_width="110dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="Some short description of this type of report."
                        android:textColor="#FF5D50"
                        android:textSize="10sp"
                        app:layout_constraintEnd_toEndOf="@+id/user_ttt"
                        app:layout_constraintStart_toStartOf="@id/user_ttt"
                        app:layout_constraintTop_toBottomOf="@id/user_ttt" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/card4"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:elevation="0dp"
                app:cardCornerRadius="16dp"
                app:layout_constraintHorizontal_chainStyle="spread_inside"
                app:layout_constraintStart_toEndOf="@+id/card3"
                app:layout_constraintTop_toTopOf="@id/card3">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/userqqq"
                        android:layout_width="16dp"
                        android:layout_height="16dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="20dp"
                        android:src="@drawable/heart__4__1"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:tint="#FF5D50" />


                    <TextView
                        android:id="@+id/user_tttt"
                        android:layout_width="110dp"
                        android:layout_height="30dp"
                        android:layout_marginTop="10dp"
                        android:text="Love report"
                        android:textColor="#C93F8D"
                        android:textSize="12sp"
                        app:layout_constraintStart_toStartOf="@id/userqqq"
                        app:layout_constraintTop_toBottomOf="@id/userqqq" />

                    <TextView
                        android:id="@+id/user_t_2222"
                        android:layout_width="110dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:text="Some short description of this type of report."
                        android:textColor="#C93F8D"
                        android:textSize="10sp"
                        app:layout_constraintEnd_toEndOf="@+id/user_tttt"
                        app:layout_constraintStart_toStartOf="@id/user_tttt"
                        app:layout_constraintTop_toBottomOf="@id/user_tttt" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
        </androidx.constraintlayout.widget.ConstraintLayout>




    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>